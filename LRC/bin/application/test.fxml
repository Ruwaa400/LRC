      <AnchorPane fx:id="LoginPane" prefHeight="593.0" prefWidth="1131.0" style="-fx-background-color: white;" AnchorPane.topAnchor="60.0">
         <children>
            <Label layoutX="890.0" layoutY="541.0" prefHeight="30.0" prefWidth="220.0" text="Language Resources Center.2019" textFill="#3a1c9f">
               <font>
